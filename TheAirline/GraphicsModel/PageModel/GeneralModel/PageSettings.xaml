<page:StandardPage x:Class="TheAirline.GraphicsModel.PageModel.GeneralModel.PageSettings"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:page="clr-namespace:TheAirline.GraphicsModel.PageModel.GeneralModel"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
	Title="PageSettings">
    <Page.Resources>
        <DataTemplate x:Key="LanguageItem">
            <DockPanel>
                <Image Source="{Binding ImageFile}" Width="20" VerticalAlignment="Bottom" RenderOptions.BitmapScalingMode="HighQuality"/>
                <TextBlock Text="{Binding Name}" Margin="5 0 0 0" TextAlignment="Left" Width="100" VerticalAlignment="Bottom"/>
            </DockPanel>
        </DataTemplate>
    </Page.Resources>
    <Grid>
        
    </Grid>
</page:StandardPage>
