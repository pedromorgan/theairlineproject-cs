<page:StandardPage x:Class="TheAirline.GraphicsModel.PageModel.PageGameModel.PageNewAirline"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:page="clr-namespace:TheAirline.GraphicsModel.PageModel.GeneralModel"
                   xmlns:c="clr-namespace:TheAirline.GraphicsModel.Converters"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
	Title="PageNewAirline">
    <Page.Resources>
        <c:StringToBrushConverter x:Key="stringToBrushConverter"></c:StringToBrushConverter>
    <DataTemplate x:Key="ColorItem">
            <WrapPanel>
                <Rectangle Width="20" Height="10" Fill="{Binding Name,Converter={StaticResource stringToBrushConverter}}" Margin="0 0 5 0"></Rectangle>
                <TextBlock Text="{Binding Name}"></TextBlock>
            </WrapPanel>
        </DataTemplate>
    </Page.Resources>
    <Grid>
        
    </Grid>
</page:StandardPage>
