<window:PopUpWindow x:Class="TheAirline.GraphicsModel.UserControlModel.PopUpWindowsModel.PopUpInviteAlliance"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:window="clr-namespace:TheAirline.GraphicsModel.UserControlModel.PopUpWindowsModel"
                    xmlns:c="clr-namespace:TheAirline.GraphicsModel.Converters"
        Title="PopUpInviteAlliance" Height="300" Width="300">
    <Window.Resources>
        <c:AirlineBrushConverter x:Key="airlineBrushConverter"></c:AirlineBrushConverter>
            <DataTemplate x:Key="AirlineItem">
            <WrapPanel>
                <Image Source="{Binding Profile.Logo}" VerticalAlignment="Bottom" RenderOptions.BitmapScalingMode="HighQuality" Width="32" Height="32" Margin="2 0 5 0"></Image>
                <TextBlock Width="150" VerticalAlignment="Bottom" Text="{Binding Profile.Name}"/>
               
            </WrapPanel>
        </DataTemplate>
    </Window.Resources>
<Grid>
        
    </Grid>
</window:PopUpWindow>
